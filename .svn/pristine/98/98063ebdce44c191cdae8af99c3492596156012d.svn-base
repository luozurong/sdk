*{
	padding: 0;
	margin: 0;
	font-family: "MILT_RG,arial,Microsoft YaHei";
	font-size: 14px;
}
body{
	background: #f7f8f9;
}
[v-cloak]{
	display: none !important;
}
a{
	text-decoration: none;
	color:#333;
}
.apply-header{
	overflow: hidden;
	width: 1100px;
	margin:0 auto;
	padding: 20px 0;
	border-bottom:1px solid #f0f0f0;
	background: #fff;
	margin-top: 40px;
}
.apply-header img{
	width: 60px;
	height:60px;
	border-radius: 100%;
	float:left;
	margin-left:10px;
}
.apply-header span{
	float:left;
}
.apply-name{
	font-size: 20px;
	color:#333;
	line-height: 60px;
	margin-left: 10px; 
	cursor: pointer;
}
.apply-edit{
	font-size: 12px;
	color:#fc9153;
	line-height: 60px;
	margin-left: 10px; 
	cursor: pointer;
}
.apply-content{
	width:1100px;
	margin:30px auto;
	padding: 0px 10px 40px 10px;
	background: #fff;
	box-sizing: border-box;
}
.apply-index{
	float:right;
	line-height: 60px;
	margin-right:10px;
}
.apply-add a{
	display: inline-block;
	width:150px;
	line-height: 50px;
	border-radius: 5px;
	background: #fc9153;
	text-align: center;
	color:#fff;
	margin:40px 0;
	cursor: pointer;
	margin-right:20px;
}
.apply-add .noAddApply{
	background: #ccc;
}
.apply-nav-span{
	border:1px solid #eee;
	position: relative;
	height:60px;
}
.apply-nav-span div{
	position: absolute;
	top:0;
	left:1px;
	z-index: 1;
	height:61px;
}
.apply-nav-span div span{
	display: inline-block;
	line-height: 60px;
	border:1px solid #eee;
	width: 150px;
	text-align: center;
	float:left;
	border-top:none;
	border-bottom:none;
	cursor: pointer;
	background: #fff;
	border-left:none;
}
.apply-nav-span div span.apply-nav-ping{
	border-left:none;
}
.apply-nav-span div span.apply-nav-active{
	color:#fc9153;
	height:61px;
}
.apply-label{
	padding-left: 10px;
}
.apply-label label{
	display: inline-block;
	margin-right:10px;
	padding:30px 0 25px 0;
}
input[name="status"],input[name="terracestatus"]{
	width: 15px;
	height: 15px;
	float:left;
	margin-top:5px;
	margin-left:4px;
}
.apply-label label span{
	float:left;
	font-size: 16px;
}
table{
	border: none;
	table-layout: fixed;
	width: 1077px;
}
thead{
	background: #f7f8f9;
	width:100%;
}
tr{
	line-height: 60px;
}
thead tr th, tbody tr td{
	text-align: left;
	padding-left: 10px;
	box-sizing: border-box;
}
.apply-nav-pop{
	border:1px solid #eee;
	border-top:none;
	margin-left: 1px;
	overflow: hidden;
}
tbody tr:nth-child(2n){
	background: #f7f8f9;
}
td{
	white-space:nowrap; 
	overflow:hidden; 
	text-overflow:ellipsis;
} 
tbody tr td:first-child{
	padding-left: 10px;
}
.apply-area-add{
	padding-left: 10px;
	cursor: pointer;
	overflow: hidden;
	display: inline-block;
}
.apply-area-add img{
	width: 24px;
	height: 24px;
	float:left;
	margin-top:17px;
}
.apply-area-add span{
	float:left;
	margin-left: 10px;
	line-height: 60px;
}
.sdkDel{
	cursor: pointer;
}
.apply-ping  table{
	border-bottom: 1px solid #f0f0f0;
	padding-top:10px;
}
.apply-ping  table:last-child{
	border-bottom: none;
}
.apply-ping  table.apply-area{
	border:none;
}
.layui-layer{
	min-width: 260px;
}
.layui-layer-dialog .layui-layer-content{
	position: relative;
	padding:20px;
	line-height:24px;
	word-break:break-all;
	overflow:hidden;
	overflow:auto;
	border-bottom: 1px solid #eee;
	margin-bottom: 10px;
}
.layui-layer .layui-layer-btn .layui-layer-btn0{
	border-color: #fc9153; 
	background-color: #fc9153; 
	color:#fff;
}
html{
	background: #f7f8f9 !important;
}