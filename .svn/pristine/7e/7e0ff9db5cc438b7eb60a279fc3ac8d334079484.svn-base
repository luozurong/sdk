*{
	padding: 0;
	margin: 0;
	font-family: "MILT_RG,arial,Microsoft YaHei";
	font-size: 14px;
}
a{
	text-decoration: none;
	color:#333;
}
.index-link{
	width: 100%;
	background: rgba(0,0,0,0.8);
}
.index-link>div{
	width:1100px;
	margin:0 auto;
	overflow: hidden;
	color:#fff;
}
.logo{
	float:left;
}
.logo span{
	font-size: 18px;
	line-height: 50px;
}
.index-nav{
	float:right;
}
.index-nav-warp,.index-nav-login{
	float:right;
}
.index-link div a{
	color:#fff;
	font-size: 18px;
}
.index-nav-warp a{
	display: inline-block;
	margin-right: 50px;
	line-height: 50px;
	font-size: 18px;
}
.index-nav-login a,.index-nav-login span{
	display: inline-block;
	line-height: 50px;
}
.register{
	margin-top:1px;
}