*{
    padding: 0;
    margin: 0;
    font-family: "MILT_RG,arial,Microsoft YaHei";
    font-size: 14px;
    box-sizing: border-box;

}
html{
    background-color:#f3f4f5;
}
div{
    display: block;
}
ul, ol{
    list-style: none;
}
a{
    text-decoration: none;
    color:#333;
}
.center{
    text-align: center;
}
.index-link{
    width: 100%;
    background: rgba(0,0,0,0.8);
}
.index-link>div{
    width:1100px;
    margin:0 auto;
    overflow: hidden;
    color:#fff;
}
.logo{
    float:left;
}
.logo img{
    float:left;
    width: 32px;
    margin-top: 10px;
    margin-right:10px;
}
.logo span{
    font-size: 18px;
    line-height: 50px;
}
.index-nav{
    float:right;
}
.index-nav-warp,.index-nav-login{
    float:right;
}
.index-link div a{
    color:#fff;
    font-size: 18px;
}
.index-nav-warp a{
    display: inline-block;
    margin-right: 50px;
    line-height: 50px;
    font-size: 18px;
}
.index-nav-login a,.index-nav-login span{
    display: inline-block;
    line-height: 50px;
}
.register{
    margin-top:1px;
}
/*ÕÒ»ØÃÜÂë*/
.password_top{
    background-color: #fff;
    border-top:1px solid #CCCCCC;
    border-bottom:1px solid #CCCCCC;
}
.password_img{
        width:50px;
        height:50px;
}
.password_text{
    width: 1100px;
    font-size: 18px;
    margin:0 auto;
    font-weight:700;
    padding:15px 15px;
    background-color:#fff;
}
.content{
    height: auto;
    width: 1100px;
    font-size: 18px;
    margin:0 auto;
    background-color:#fff;
    margin-top: 30px;
    margin-bottom:30px;
}
.content_top{
    /*min-height: 500px;*/
    height: auto;
}
.content_nav{
    height:34px;
    margin:30px 0;
    line-height: 33px;
    font-size:18px;
}
.content_top li{
    float:left;
    padding-left: 66px;
}
.nav_text{
    font-size: 14px;
    padding-bottom: 12px;
    padding:20px;
    padding-left:32%;
    /*text-align: center;*/
}
.pass_input{
    display: block;
    position: relative;
    float: left;
    height: 32px;
    width: 328px;
    padding: 11px 10px;
    margin-right: 10px;
    border: 1px solid #ccc;
    font-size: 14px;
    /*color: #666;*/
    -webkit-transition: .3s;
    -moz-transition: .3s;
    -o-transition: .3s;
    transition: .3s;
    background: #ffffff;
}
.import_iphone{
    top: 6px;
    left: 10px;
    line-height: 30px;
    font-size: 14px;
}

.pass_security{
    width:134px;
    height:45px;
}
.pass_a{
    text-decoration: none;
    color: #2b78e4;
    line-height: 36px;
    padding-left:23px;
}
.pass_img{
    float: left;
    width: 100px;
    height:45px;
    border: 1px solid #ccc;
    margin-left: 10px;
}
::-webkit-input-placeholder {
    color: #cccccc;
}
:-moz-placeholder {/* Firefox 18- */
    color: #cccccc;
}
::-moz-placeholder{/* Firefox 19+ */
    color: #cccccc;
}
:-ms-input-placeholder {
    color: #cccccc;
}
.pass_button{
    display: block;
    height: 40px;
    font-size: 16px;
    font-weight: 700;
    cursor: pointer;
    color: #fff;
    background-image: none;
    border-radius: 3px;
    border: 0;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    transition: .3s;
    -moz-transition: .3s;
    -o-transition: .3s;
    -webkit-transition: .3s;
    background-color: #fc9153;
    width: 350px;
    margin-top:20px;
}
.verify-method {
    font-size: 14px;
    font-weight: 700;
    line-height: 30px;
}
.select-question{
    width: 325px;
    _width: 340px;
    border: #ccc 1px solid;
    height: 38px;
    position: relative;
}
#pass-button-new1 {
    width: 120px;
    height: 36px;
    border: 1px solid #ccc;
    background: #f7f7f7;
    color: #666;
    line-height: 36px;
}
.pass_button_timer{
    cursor: pointer;
    float: left;
    padding-left:24px;
}
.nav_iphone{
    padding-left: 32%;
}
.nav_auth{
    padding-left:32%;
}
.nav_submit{
    padding-left:32%;
    padding-bottom: 20px;
}
.nav_text>p{
    font-size:20px;
}


