*{
	padding: 0;
	margin: 0;
	font-family: "MILT_RG,arial,Microsoft YaHei";
	font-size: 14px;
}
a{
	text-decoration: none;
	color:#333;
}
[v-cloak]{
	display: none !important;
}
#indexWrap>div{
	min-width: 85px;
}
.index-link{
	width: 100%;
	background: rgba(0,0,0,0.8);
}
.index-link>div{
	width:1100px;
	margin:0 auto;
	overflow: hidden;
	color:#fff;
}
.logo{
	float:left;
}
.logo img{
	float:left;
	width: 32px;
	height:32px;
	margin-top: 10px;
	margin-right:10px;
}
.logo span{
	font-size: 18px;
	line-height: 50px;
}
.index-nav{
	float:right;
}
.index-nav-warp,.index-nav-login{
	float:right;
}
.index-link div a{
	color:#fff;
	font-size: 18px;
	cursor: pointer;
}
.index-nav-warp a{
	display: inline-block;
	margin-right: 50px;
	line-height: 50px;
	font-size: 18px;
	cursor: pointer;
}
.index-nav-login a,.index-nav-login span{
	display: inline-block;
	line-height: 50px;
}
.register{
	margin-top:1px;
}
.swiper-slide{
	width:100%;
	height:650px;
	background: #ccc;
}
.index-footer{
	background: #f7f8f9;
	padding:40px 0;
}
.index-footer>div{
	width: 1100px;
	margin: 0 auto;
}
.footer-floor1>div{
	display: inline-block;
	line-height: 36px;
	margin-right:200px;
}
.footer-floor-key{
	font-size: 16px;
	color:#333;
}
.footer-floor1 div div a{
	font-size: 12px;
	color: #999;
}
.footer-floor2{
	text-align: center;
	color: #999;
	padding-top: 30px;
}
.index-wrap{
	width: 1100px;
	margin: 50px auto;
	overflow: hidden;
}
.index-wrap>div{
	width: 30%;
	display: inline-block;
	float:left;
	overflow: hidden;
	background: #f7f8f9;
	padding-bottom: 30px;
}
.wrap-floor2{
	margin:0 5%;
}
.index-wrap div div{
	width: 50%;
	float:left;
}
.index-wrap>div>p{
	line-height: 60px;
	padding-left:20px;
	border-bottom:2px solid #fff;
	font-size: 20px;
}
.index-wrap .wrap-word1{
	border-color: #ff22ac;
	color:#ff22ac;
}
.index-wrap .wrap-word2{
	border-color: #0055ff;
	color: #0055ff;
}
.index-wrap .wrap-word3{
	border-color: #cc22ff;
	color: #cc22ff;
}
.index-wrap div div img{
	display: block;
	width: 50px;
	height: 50px;
	/*border:1px solid #ccc;*/
	margin: 20px auto 0;
}
.index-wrap div div p{
	text-align: center;
	color:#888;
	font-size: 14px;
	line-height: 20px;
}
.index-wrap div div p.wrap-work-key{
	font-size: 16px;
	color:#333;
	line-height: 30px;
}
.index-container{
	width: 1100px;
	margin: 0 auto 40px;
	overflow: hidden;
}

.index-container>div{
	display: inline-block;
	
	float:left;
}
.index-container>div img{
	width: 500px;
}
.container-word{
	width:590px;
}
.container-app{
	text-align: center;
	font-size: 42px;
	margin-top:200px;
	line-height: 80px;
	color:#333;
}
.container-app-word{
	text-align: center;
	line-height: 40px;
	font-size: 24px;
	color:#888;
	letter-spacing: 2px;
}